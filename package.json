{
    "name": "my-favourite-league",
    "version": "0.0.1",
    "description": "An application to keep track of your favorite or strongest champions in the MOBA game called League of Legends",
    "keywords": [
        "league",
        "of",
        "legends",
        "league",
        "lol",
        "champion",
        "list"
    ],
    "author": "Fabian Kresler",
    "license": "MIT",
    "main": "index.js",
    "scripts": {
        "start": "npm run build && node server.js",
        "start:dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
        "start:backend": "node server.js",
        "start:frontend": "react-scripts start",
        "lint": "eslint --ext .js,.jsx,.ts,.tsx ./",
        "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix ./",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "test:all": "npm run test -- --watchAll",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": "airbnb",
        "env": {
            "browser": true,
            "node": true,
            "jest": true
        },
        "settings": {
            "import/resolver": {
                "node": {
                    "paths": [
                        "src"
                    ]
                }
            }
        }
    },
    "dependencies": {
        "axios": "^0.19.0",
        "cross-fetch": "^3.0.2",
        "eslint": "^5.16.0",
        "eslint-config-airbnb": "^17.1.1",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.16.0",
        "express": "^4.16.4",
        "isomorphic-fetch": "^2.2.1",
        "js-cookie": "^2.2.1",
        "lodash": "^4.17.15",
        "morgan": "^1.9.1",
        "path": "^0.12.7",
        "prop-types": "^15.7.2",
        "react": "^16.10.2",
        "react-contenteditable": "^3.3.2",
        "react-dnd": "^9.4.0",
        "react-dnd-html5-backend": "^9.4.0",
        "react-dom": "^16.10.2",
        "react-redux": "7.0.3",
        "react-router-dom": "5.0.1",
        "react-scripts": "3.0.1",
        "redux": "^4.0.4",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.3.0",
        "styled-components": "^4.4.0"
    },
    "proxy": "http://localhost:3001",
    "devDependencies": {
        "concurrently": "^4.1.2"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
